{
    "version": 2,
    "builds": [
        {
            "src": "mental_health_bot/mental_health_site/wsgi.py",
            "use": "@vercel/python",
            "config": { 
                "maxLambdaSize": "15mb", 
                "runtime": "python3.9",
                "excludeFiles": "**.csv"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "mental_health_bot/mental_health_site/wsgi.py"
        }
    ]
}